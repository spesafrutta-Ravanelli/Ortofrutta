{
  "name": "ortofrutta-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Sito web per rivendita ortofrutticola - Vue 3 + Vite + Pinia",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node copy-volantino.js",
    "build:simple": "vite build --config vite.config.simple.js && node copy-volantino.js",
    "build:vercel": "vite build && node copy-volantino.js",
    "preview": "vite preview",
    "clean": "rm -rf node_modules dist node_modules/.vite",
    "fix": "powershell -ExecutionPolicy Bypass -File fix-build.ps1"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "sass": "^1.70.0",
    "terser": "^5.46.0",
    "vite": "^5.0.12"
  }
}
